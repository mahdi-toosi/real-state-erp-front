{
	"name": "rhino-front",
	"version": "0.1.118",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"dev": "vite",
		"build": "cross-env NODE_ENV=production vue-tsc --noEmit && vite build",
		"build:prod": "cross-env NODE_ENV=production vite build",
		"build:stg": "cross-env NODE_ENV=staging vite build --mode staging",
		"serve": "vite preview",
		"lint": "eslint --ext .js,.ts,.vue .",
		"lint:fix": "yarn lint --fix",
		"lint:markup": "vue-tsc --noEmit",
		"lint:style": "stylelint **/*.{scss,css,vue}",
		"lint:style:fix": "yarn lint:style --fix",
		"format:check": "prettier .",
		"format:fix": "prettier -u --write .",
		"generate:changelog": "standard-version --skip.commit --skip.tag",
		"release": "standard-version && git push origin main --follow-tags",
		"merge": "node scripts/create-merge-request.js"
	},
	"dependencies": {
		"axios": "^1.1.2",
		"vue": "^3.2.13",
		"dayjs": "^1.10.7",
		"jalaliday": "^2.3.0",
		"nprogress": "^0.2.0",
		"primevue": "^3.12.0",
		"primeicons": "^6.0.1",
		"vue-router": "^4.0.11",
		"vue-sidebar-menu": "^5.1.1",
		"vue-advanced-cropper": "^2.8.1",
		"@ckeditor/ckeditor5-vue": "^4.0.1",
		"@vuelidate/core": "^2.0.0-alpha.27",
		"vue3-persian-datetime-picker": "^1.2.2",
		"@vuelidate/validators": "^2.0.0-alpha.23",
		"ckeditor5-custom-build": "file:packages/ckeditor5-35.3.1-kphpybewelsu"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.15.8",
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"@vitejs/plugin-vue": "^3.1.2",
		"@vue/compiler-sfc": "^3.2.20",
		"@vue/eslint-config-prettier": "6.*",
		"@vue/eslint-config-typescript": "^11.0.2",
		"autoprefixer": "^10.3.7",
		"cross-env": "^7.0.3",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^9.6.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"postcss": "^8.3.9",
		"postcss-html": "^1.5.0",
		"prettier": "^2.4.1",
		"rollup-plugin-analyzer": "^4.0.0",
		"rollup-plugin-visualizer": "^5.6.0",
		"standard-version": "^9.3.2",
		"stylelint": "^14.13.0",
		"stylelint-config-recommended": "^9.0.0",
		"stylelint-config-recommended-vue": "^1.4.0",
		"stylelint-config-standard": "^28.0.0",
		"tailwindcss": "^3.0.15",
		"typescript": "^4.4.4",
		"vite": "^3.1.6",
		"vite-plugin-eslint": "^1.3.0",
		"vue-tsc": "^1.0.3"
	},
	"resolutions": {
		"@babel/core": "^7.0.0",
		"rollup": "^2.0.0"
	}
}
